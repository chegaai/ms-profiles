  "description": "Profiles microservice",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run lint && npm run build && npm test -- -b",
    "clean": "rm -rf ./dist",
    "test": "mocha --exit -r ts-node/register --no-warnings test/**/*.ts",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "start:debug": "tsc && concurrently -s all \"tsc -w\" \"DEBUG=expresso:* nodemon dist/index.js\"",
    "lint": "eslint src/**/*.ts"
  },
